<?xml version="1.0"?>
<application xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="6">
  <application-name>yasl-ear</application-name>
  <module>
    <web>
      <web-uri>yasl-war.war</web-uri>
      <context-root>/yasl</context-root>
    </web>
  </module>
  <library-directory>lib</library-directory>
  <env-entry>
    <description>Version Information</description>
    <env-entry-name>Version Information</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>local-dev, rev9999</env-entry-value>
  </env-entry>
</application>
